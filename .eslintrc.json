{
    "env": {
        "browser": true,
        "node": true,
        "amd": true,
        "es2021": true
    },
    "ignorePatterns": ["**/dist/*", "*/node_modules/*"],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
    }
}
